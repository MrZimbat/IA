<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IA">
        <activity
            android:name=".Controller.MenuActivity"
            android:exported="false" />
        <activity
            android:name=".Controller.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".Controller.AddMedActivity"
            android:exported="false" />
        <activity
            android:name=".Controller.MedInfoActivity"
            android:exported="false" />
        <activity
            android:name=".Controller.MedProfileActivity"
            android:exported="false" />
        <activity
            android:name=".Controller.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>